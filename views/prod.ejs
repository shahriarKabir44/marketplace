                        <h5>Title</h5>
                        <p>Text</p>
                    </div>
                </div>
            </div>
            <a class="carousel-control-prev" href="#showprod-carousel" data-slide="prev" role="button">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#showprod-carousel" data-slide="next" role="button">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>
        </div>
    </div>
    <div class="col-sm-12 col-lg-4">
        <table class="table table-light table-hover">
            <tbody>
                <tr>
                    <td>
                        <p>Type: <%= data.type %></p>
                    </td>
                    <td>
                        <p></p>
                    </td>
                </tr>
                <tr>
                    <td>
                        <p class="text-success">Price: </p>
                        <p class="text-success" id="prc"><%= data.askedPrc %></p>
                    </td>
                </tr>
                <tr>
                    <td>
                        <p>Posted On: <%= data.date %></p>
                    </td>
                    <td>
                        <p></p>
                    </td>
                </tr>
                <tr>
                    <td>
                        <p>Details: </p>
                    </td>
                    <td>
                        <p><%= data.details %></p>
                    </td>
                </tr>

            </tbody>
        </table>
        <button class="btn btn-success " id="General1">Add To
            cart</button>
        <button class="btn btn-danger " id="myProd1">Remove Ad!</button>
        <button class="btn btn-danger " id="myProd2">Remove From cart!</button>
        <button class="btn btn-success " id="myProd3">Confirm Buy!</button>
    </div>
    <div class="col-sm-10 col-lg-3">
        <div id="buyers_list">
            <h4>Buyers' List</h4>
            <div id="byrList" style="height:250px; overflow-y:scroll">
                <table border="1px">
                    <thead>
                        <th>Name</th>
                        <th>Price</th>
                        <th>Date</th>
                    </thead>
                    <tbody id="tbd_custs"></tbody>
                </table>
            </div>
        </div>
    </div>

</div>
<!--main prod showcase end -->

